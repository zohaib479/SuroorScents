generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  address   String
  phone     String
  product   String
  quantity  Int
  price     Float
  createdAt DateTime @default(now())
}
